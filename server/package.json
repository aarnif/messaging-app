{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:db": "docker compose -f docker-compose.yml up",
    "populate:db": "POPULATE_DB=true tsx src/populateDatabase.ts",
    "generate": "graphql-codegen --config codegen.ts",
    "dev": "npm run generate && NODE_ENV=development nodemon --exec tsx src/index.ts",
    "test": "NODE_ENV=test tsx --test src/tests/*.test.ts",
    "test:coverage": "NODE_ENV=test NODE_V8_COVERAGE=coverage tsx --experimental-test-coverage --test src/tests/*.test.ts",
    "typecheck": "tsc",
    "lint": "eslint .",
    "prod": "npm run generate && NODE_ENV=production tsx src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "graphql-redis-subscriptions": "^2.7.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-tag": "^2.12.6",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "umzug": "^3.8.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@graphql-codegen/typescript-resolvers": "^5.0.0",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.36.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1"
  }
}
